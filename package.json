{
  "name": "startify",
  "displayName": "Startify",
  "version": "0.0.11",
  "description": "A customizable and beautiful new tab page for your browser.",
  "author": "PawelGawlikDev",
  "scripts": {
    "build": "plasmo build --hoist",
    "build:firefox": "plasmo build --hoist --target=firefox-mv3 --env=.env.firefox",
    "build:firefox-mv2": "plasmo build --hoist --target=firefox-mv2 --env=.env.firefox",
    "dev": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv3 --env=.env.firefox",
    "dev:firefox-mv2": "plasmo dev --target=firefox-mv2 --env=.env.firefox",
    "e2e": "playwright test",
    "e2e:debug": "cross-env DEBUG=true playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "format": "prettier . --write --config .prettierrc.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "plasmo package",
    "prepare": "husky",
    "release": "standard-version",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "storybook": "ladle serve --theme dark"
  },
  "dependencies": {
    "@ladle/react": "^5.0.1",
    "@radix-ui/react-label": "^2.1.2",
    "@tabler/icons-react": "^3.30.0",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "favicon-fetch": "^1.0.0",
    "motion": "^12.4.7",
    "plasmo": "^0.90.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.0.2",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@plasmohq/rps": "^1.8.7",
    "@plasmohq/storage": "1.15.0",
    "@playwright/test": "^1.50.1",
    "@types/chrome": "^0.0.306",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "commitlint": "^19.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.7"
  },
  "manifest": {
    "browser_specific_settings": {
      "gecko": {
        "id": "{d5e5fe42-da08-4573-8ffd-6075ae68c834}"
      }
    },
    "chrome_settings_overrides": {
      "homepage": "$HOMEPAGE"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage",
      "activeTab"
    ]
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "tailwindcss"
      ]
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "msw",
      "sharp"
    ]
  }
}
