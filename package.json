{
  "name": "startify",
  "displayName": "Startify",
  "version": "0.0.1",
  "description": "A customizable and beautiful new tab page for your browser.",
  "author": "PawelGawlikDev",
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:plasmo": "plasmo build",
    "dev": "run-p dev:*",
    "dev:next": "next dev --port 1947",
    "dev:plasmo": "plasmo dev",
    "e2e": "playwright test",
    "e2e:debug": "cross-env DEBUG=true playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "format": "prettier . --write --config .prettierrc.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "plasmo package",
    "prepare": "husky",
    "release": "standard-version",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.0",
    "@tabler/icons-react": "^3.20.0",
    "clsx": "^2.1.1",
    "color": "^4.2.3",
    "dexie": "^4.0.9",
    "dexie-react-hooks": "^1.1.7",
    "favicon-fetch": "^1.0.0",
    "framer-motion": "^11.11.9",
    "lit": "^3.2.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.453.0",
    "next": "15.0.1",
    "plasmo": "0.89.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.2.10",
    "tailwind-merge": "^2.5.4",
    "vite": "^5.4.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@next/eslint-plugin-next": "^15.0.1",
    "@plasmohq/rps": "1.8.7",
    "@plasmohq/storage": "1.12.0",
    "@playwright/test": "^1.48.1",
    "@types/chrome": "0.0.279",
    "@types/node": "22.7.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/eslint-plugin": "^1.1.7",
    "@vitest/ui": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-plugin-playwright": "^2.0.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "standard-version": "^9.5.0",
    "storybook": "^8.3.6",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.11.0",
    "vitest": "^2.1.3"
  },
  "manifest": {
    "browser_specific_settings": {
      "gecko": {
        "id": "{d5e5fe42-da08-4573-8ffd-6075ae68c834}"
      }
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage"
    ]
  }
}
