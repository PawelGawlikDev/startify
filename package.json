{
  "name": "startify",
  "displayName": "Startify",
  "version": "0.0.8",
  "description": "A customizable and beautiful new tab page for your browser.",
  "author": "PawelGawlikDev",
  "scripts": {
    "build": "plasmo build",
    "dev": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv3 --env=.env.firefox",
    "dev:firefox-mv2": "plasmo dev --target=firefox-mv2 --env=.env.firefox",
    "e2e": "playwright test",
    "e2e:debug": "cross-env DEBUG=true playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "format": "prettier . --write --config .prettierrc.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "plasmo package",
    "prepare": "husky",
    "release": "standard-version",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.1",
    "@tabler/icons-react": "^3.26.0",
    "clsx": "^2.1.1",
    "color": "^4.2.3",
    "dexie": "^4.0.10",
    "dexie-react-hooks": "^1.1.7",
    "favicon-fetch": "^1.0.0",
    "motion": "^12.0.0-alpha.2",
    "plasmo": "0.89.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.5",
    "tailwind-merge": "^2.6.0",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@plasmohq/rps": "1.8.7",
    "@plasmohq/storage": "1.13.0",
    "@playwright/test": "^1.49.1",
    "@types/chrome": "0.0.289",
    "@types/node": "22.10.1",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.24",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "commitlint": "^19.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.19.0",
    "vitest": "^2.1.8"
  },
  "manifest": {
    "browser_specific_settings": {
      "gecko": {
        "id": "{d5e5fe42-da08-4573-8ffd-6075ae68c834}"
      }
    },
    "chrome_settings_overrides": {
      "homepage": "$HOMEPAGE"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage"
    ]
  }
}
