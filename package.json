{
  "name": "startify",
  "displayName": "Startify",
  "version": "1.5.1",
  "private": true,
  "description": "A customizable and beautiful new tab page for your browser.",
  "author": "PawelGawlikDev",
  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "e2e": "playwright test",
    "e2e:debug": "cross-env DEBUG=true playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "fix": "pnpm '/^fix:.*/'",
    "fix:eslint": "eslint . --fix",
    "fix:prietter": "prettier . --write --config .prettierrc.mjs",
    "postinstall": "wxt prepare",
    "lint": "pnpm '/^lint:.*/'",
    "lint:eslint": "eslint .",
    "lint:prietter": "prettier . --check --config .prettierrc.mjs",
    "prepare": "husky",
    "release": "standard-version",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/eslint-plugin": "^1.4.3",
    "@vitest/ui": "^4.0.13",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "commitlint": "^20.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "standard-version": "^9.5.0",
    "svgo": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vitest": "^4.0.13",
    "wxt": "^0.20.11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "msw",
      "sharp",
      "spawn-sync"
    ],
    "overrides": {
      "vite": "^7.1.1"
    }
  }
}
