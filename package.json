{
  "name": "startify",
  "displayName": "Startify",
  "version": "1.0.4",
  "private": true,
  "description": "A customizable and beautiful new tab page for your browser.",
  "author": "PawelGawlikDev",
  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "format:check": "prettier . --check --config .prettierrc.mjs",
    "format:fix": "prettier . --write --config .prettierrc.mjs",
    "postinstall": "wxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release": "standard-version",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "motion": "^12.12.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.0",
    "@vitest/ui": "^3.1.4",
    "@wxt-dev/module-react": "^1.1.3",
    "commitlint": "^19.8.1",
    "eslint": "^9.27.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "standard-version": "^9.5.0",
    "svgo": "^3.3.2",
    "tailwind-merge": "^3.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vitest": "^3.1.4",
    "wxt": "^0.20.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "spawn-sync"
    ]
  }
}
